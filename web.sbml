=begin sbml

=object web: id=${web-id}.web, filename=${dir-path}/postcode.html, base-url=http://localhost, \
            javascript-bridge="__$_bridge", uses-browser-user-agent=yes, \
            content-script@start=${dir-path}/cleanup.js, \
            script-when-loaded=${web-prefix}__on_web_loaded, \
            style=web

=object progress: type=loading, owner=${web-id}.web, style=progress_web

=end sbml
